{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "deepair_frontend/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },
    {
      "src": "/",
      "dest": "/deepair_frontend/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/deepair_frontend/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/deepair_frontend/index.html"
    }
  ]
}

